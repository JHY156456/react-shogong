thForm = ({ type, form, onChange, onSubmit, error,changeAddress }) => {\r\n  const text = textMap[type];\r\n  return (\r\n    <AuthFormBlock>\r\n      <h3>{text}</h3>\r\n      {(function () {\r\n        if (type === \"first\")\r\n          return (\r\n            <FirstRegisterPage\r\n              onSubmit={onSubmit}\r\n              form={form}\r\n              onChange={onChange}\r\n              error={error}\r\n            />\r\n          );\r\n        else if (type === \"second\")\r\n          return (\r\n            <SecondRegisterPage\r\n              onSubmit={onSubmit}\r\n              form={form}\r\n              changeAddress={changeAddress}\r\n              onChange={onChange}\r\n              error={error}\r\n            />\r\n          );\r\n        else if (type === \"third\")\r\n          return (\r\n            <ThirdRegisterPage\r\n              onSubmit={onSubmit}\r\n              form={form}\r\n              onChange={onChange}\r\n              error={error}\r\n            />\r\n          );\r\n        else if (type === \"manufacturer\")\r\n          return (\r\n            <ManufacturerStory onSubmit={onSubmit} form={form} error={error} />\r\n          );\r\n        else if (type === \"registeredbasicinformation\")\r\n          return (\r\n            <RegisteredBasicInformation\r\n              onSubmit={onSubmit}\r\n              form={form}\r\n              error={error}\r\n            />\r\n          );\r\n      })()}\r\n    </AuthFormBlock>\r\n  );\r\n};\r\n\r\nexport default RegisterAuthForm;\r\n"},"lineCount":null}},"error":null,"hash":"2c6ef2c1891fd24d36764594129d4094","cacheData":{"env":{}}}