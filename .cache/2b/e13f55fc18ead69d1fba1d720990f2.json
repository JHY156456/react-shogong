port styled from \"styled-components\";\r\nimport posed from \"react-pose\";\r\nimport useOnClickOutside from \"../../common/UseOnClickOutside\";\r\n\r\nconst modalBackgroundPoses = {\r\n  open: {\r\n    background: \"rgba(0, 0, 0, 0.2)\",\r\n    applyAtStart: {\r\n      display: \"block\"\r\n    }\r\n  },\r\n  closed: {\r\n    background: \"rgba(0, 0, 0, 0)\",\r\n    applyAtEnd: {\r\n      display: \"none\"\r\n    }\r\n  }\r\n};\r\n\r\nconst ModalBackground = styled(posed.div(modalBackgroundPoses))`\r\n  position: fixed;\r\n  top: 0;\r\n  left: 0;\r\n  width: 100%;\r\n  height: 100%;\r\n  z-index: 10;\r\n`;\r\nconst CalendarModalBackground = styled(posed.div(modalBackgroundPoses))`\r\n\r\n`;\r\nconst modalPoses = {\r\n  open: {\r\n    opacity: 1,\r\n    transition: {\r\n      opacity: {\r\n        type: \"tween\",\r\n        duration: 200\r\n      }\r\n    }\r\n  },\r\n  closed: {\r\n    opacity: 0,\r\n    transition: {\r\n      opacity: {\r\n        type: \"tween\",\r\n        duration: 200\r\n      }\r\n    }\r\n  }\r\n};\r\n\r\nconst Modal = styled(posed.div(modalPoses))`\r\n  position: fixed;\r\n  background: white;\r\n  width: 700px;\r\n  height: 600px;\r\n  overflow: scroll;\r\n  top: 50%;\r\n  left: 50%;\r\n  transform: translate(-50%, -50%);\r\n  border-radius: 10px;\r\n  padding: 20px;\r\n  box-shadow: 0 2px 4px 0 rgba(50, 50, 93, 0.1);\r\n`;\r\nconst CalendarModal = styled(posed.div(modalPoses))`\r\n  position: fixed;\r\n  background: white;\r\n  left: 50%;\r\n  transform: translate(-50%, -50%);\r\n  border-radius: 10px;\r\n  box-shadow: 0 2px 4px 0 rgba(50, 50, 93, 0.1);\r\n`;\r\nconst modal = ({ isOpen, toggle, children }) => {\r\n  const ref = useRef();\r\n\r\n  useOnClickOutside(ref, () => toggle(false));\r\n  return (\r\n    <div>\r\n        {(function() {\r\n        if (toggle === \"toggleModal\")\r\n          return (\r\n            <ModalBackground\r\n              initialPose=\"closed\"\r\n              pose={isOpen ? \"open\" : \"closed\"}\r\n            >\r\n              <Modal ref={ref}>{children}</Modal>\r\n            </ModalBackground>\r\n          );\r\n        else (toggle === \"setIsCalendarOpen\")\r\n          return (\r\n            <CalendarModalBackground\r\n              initialPose=\"closed\"\r\n              pose={isOpen ? \"open\" : \"closed\"}\r\n            >\r\n              <CalendarModal ref={ref}>{children}</CalendarModal>\r\n            </CalendarModalBackground>\r\n          );\r\n      })()}\r\n    </div>\r\n  );\r\n};\r\nexport default modal;\r\n"},"lineCount":null}},"error":null,"hash":"ecdb0c73c45d74ef5542646ff5a3e13c","cacheData":{"env":{}}}